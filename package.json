{
  "name": "simple",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "MHA",
  "private": true,
  "scripts": {
    "prestart": "FORCE_COLOR=1 babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --race --parallel dev csslint:watch jslint:watch test:watch start:mockapi",
    "dev": "cross-env NODE_ENV=development FORCE_COLOR=1 babel-node buildScripts/srcServer.js",
    "jslint": "esw webpack.config.* src buildScripts --color",
    "jslint:watch": "npm run jslint -- --watch",
    "csslint": "stylelint src/**/*.*css --color --formatter 'verbose'",
    "csslint:watch": "onchange 'src/**/*.*css' -- npm run csslint",
    "security:check": "nsp check",
    "localtunnel": "lt --port 8089",
    "share": "npm-run-all --race --parallel dev localtunnel",
    "unit": "cross-env BABEL_ENV=test karma start karma.conf",
    "test": "npm run unit -- --single-run",
    "test:watch": "npm run unit -- --watch",
    "generate:mock-data": "FORCE_COLOR=1 babel-node buildScripts/generateMockData",
    "prestart:mockapi": "npm run generate:mock-data",
    "start:mockapi": "json-server --watch src/api/db.json --port 8090",
    "clean:dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean:dist test jslint csslint",
    "build": "cross-env NODE_ENV=production FORCE_COLOR=1 babel-node buildScripts/build.js",
    "postbuild": "FORCE_COLOR=1 babel-node buildScripts/distServer.js",
    "vue:dev": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "vue:build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "vue": "^2.4.4",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-latest": "6.16.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chalk": "1.1.3",
    "compression": "^1.7.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-watch": "^3.1.2",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.3.0",
    "json-schema-faker": "^0.5.0-rc11",
    "json-server": "^0.12.0",
    "karma": "^1.7.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.5.0",
    "karma-sinon-chai": "^1.3.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.5",
    "localtunnel": "^1.8.3",
    "mocha": "^4.0.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "nsp": "^2.8.1",
    "onchange": "^3.2.1",
    "phantomjs-prebuilt": "^2.1.16",
    "phantomjs-shim": "^0.1.0",
    "postcss-loader": "^2.0.8",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.0",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-md5-hash": "0.0.5"
  }
}
